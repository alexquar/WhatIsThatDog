{
  "expo": {
    "name": "What's that Dog?!",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#fbfdfd"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#fbfdfd"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fbfdfd"
        }
      ],
      [
        "expo-camera",
        {
          "photosPermission": "Allow What's that dog to access your photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
